{
	"name": "sveltekit-minimalist-template",
	"version": "0.1.0",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PaulioRandall/sveltekit-minimalist-template.git"
	},
	"scripts": {
		"clean": "rm -rf .svelte-kit",
		"fmt": "prettier --log-level warn --write .",
		"build": "npm run fmt && vite build",
		"dev": "npm run fmt && vite dev --port 3000",
		"preview": "npm run build && vite preview --port 4000",
		"commit": "npm run build && cat ./ready-to-ship.txt",
		"================================================================": "",
		"auto-build": "vite build"
	},
	"devDependencies": {
		"@paulio/p69-svelte": "1.0.0-rc-3",
		"@paulio/p69-util": "1.0.0-rc-2",
		"@sveltejs/adapter-auto": "3.3.1",
		"@sveltejs/kit": "2.15.2",
		"prettier": "3.4.2",
		"svelte": "5.17.1",
		"vite": "6.0.7"
	}
}
